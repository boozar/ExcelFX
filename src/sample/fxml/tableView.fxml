<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0"
      prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="sample.controlls.TableViewControl">
	<children>
		<HBox prefHeight="57.0" prefWidth="600.0">
			<children>
				<Button fx:id="btnAdd" minWidth="100.0" mnemonicParsing="false" onAction="#actionBtnPressed" text="Add"
				        textAlignment="CENTER">
					<HBox.margin>
						<Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
					</HBox.margin>
				</Button>
				<Button fx:id="btnUpdate" defaultButton="true" minWidth="100.0" mnemonicParsing="false"
				        onAction="#actionBtnPressed" text="Update" textAlignment="CENTER">
					<HBox.margin>
						<Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
					</HBox.margin>
				</Button>
				<Button fx:id="btnDelete" minWidth="100.0" mnemonicParsing="false" onAction="#actionBtnPressed"
				        text="Delete" textAlignment="CENTER">
					<HBox.margin>
						<Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
					</HBox.margin>
				</Button>
			</children>
		</HBox>
		<AnchorPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
			<children>
				<TableView fx:id="tblView" layoutX="113.0" layoutY="-7.0" prefHeight="200.0" prefWidth="200.0"
				           AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0"
				           AnchorPane.topAnchor="0.0">
					<columns>
						<TableColumn fx:id="tblId" maxWidth="60.0" minWidth="60.0" prefWidth="60.0" text="Identific"/>
						<TableColumn fx:id="tblType" maxWidth="250.0" minWidth="250.0" prefWidth="250.0" text="Type"/>
						<TableColumn fx:id="tblManufacturer" maxWidth="100.0" minWidth="100.0" prefWidth="100.0"
						             text="Manufactur"/>
						<TableColumn fx:id="tblSpecification" minWidth="100.0" prefWidth="200.0" text="Specification"/>
						<TableColumn fx:id="tblRetail" maxWidth="60.0" minWidth="60.0" prefWidth="60.0" text="Retail"/>
						<TableColumn fx:id="tblWholesale" maxWidth="60.0" minWidth="60.0" prefWidth="60.0"
						             text="Whole"/>
						<TableColumn fx:id="tblDealer" maxWidth="60.0" minWidth="60.0" prefWidth="60.0" text="Dealer "/>
					</columns>
					<columnResizePolicy>
						<TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
					</columnResizePolicy>
				</TableView>
			</children>
		</AnchorPane>
		<AnchorPane prefHeight="46.0" prefWidth="600.0">
			<children>
				<Label fx:id="lblQuantity" layoutX="14.0" layoutY="119.0" maxWidth="1.7976931348623157E308"
				       prefHeight="21.0" prefWidth="571.0" text="Quantity products" AnchorPane.bottomAnchor="15.0"
				       AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="10.0"/>
			</children>
		</AnchorPane>
	</children>
</VBox>
